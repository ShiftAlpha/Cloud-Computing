
@{
    ViewBag.Title = "INDEX";
}

<h2>INDEX</h2>

<table class="table">
    <tr>
        <th>
            <text>Job Card Number</text>
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JOB_CARD_NO)
            </td>

            <td>
                @Html.ActionLink("Generate Invoice", "InvoiceDetails", new { id = item.JOB_CARD_NO })
            </td>
        </tr>
    }

</table>